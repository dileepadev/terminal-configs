{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{SECRET_DEFAULT_PROFILE}",
    "keybindings": [
        {
            "id": "Terminal.CopyToClipboard",
            "keys": "ctrl+c"
        },
        {
            "id": "Terminal.FindText",
            "keys": "ctrl+shift+f"
        },
        {
            "id": "Terminal.PasteFromClipboard",
            "keys": "ctrl+v"
        },
        {
            "id": "Terminal.DuplicatePaneAuto",
            "keys": "alt+shift+d"
        }
    ],
    "newTabMenu": [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": {
        "defaults": {},
        "list": [
            {
                "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
                "guid": "{SECRET_GUID_WINDOWS_POWERSHELL}",
                "hidden": false,
                "name": "Windows PowerShell"
            },
            {
                "commandline": "%SystemRoot%\\System32\\cmd.exe",
                "guid": "{SECRET_GUID_COMMAND_PROMPT}",
                "hidden": false,
                "name": "Command Prompt"
            },
            {
                "guid": "{SECRET_GUID_AZURE_CLOUD_SHELL}",
                "hidden": false,
                "name": "Azure Cloud Shell",
                "source": "Windows.Terminal.Azure"
            },
            {
                "guid": "{SECRET_GUID_GIT_BASH}",
                "hidden": false,
                "name": "Git Bash",
                "source": "Git"
            },
            {
                "guid": "{SECRET_GUID_POWERSHELL}",
                "hidden": false,
                "name": "PowerShell",
                "source": "Windows.Terminal.PowershellCore"
            },
            {
                "colorScheme": "Dark+",
                "commandline": "\"C:\\Program Files\\PowerShell\\7\\pwsh.exe\"  -NoLogo",
                "font": {
                    "face": "CaskaydiaCove Nerd Font"
                },
                "guid": "{SECRET_GUID_DILEEPADEV}",
                "hidden": false,
                "icon": "C:\\Users\\dileepadev\\OneDrive\\Documents\\PowerShell\\dileepadev_icon.png",
                "name": "dileepadev",
                "opacity": 96,
                "padding": "12",
                "startingDirectory": "%USERPROFILE%",
                "useAcrylic": true
            }
        ]
    },
    "schemes": [],
    "themes": []
}