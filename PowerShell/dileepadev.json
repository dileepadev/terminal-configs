{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}(Admin){{end}} {{.PWD}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "@dileepadev ",
          "foreground": "#15aefa",
          "type": "session",
          "style": "plain"
        },
        {
          "foreground": "#fa5fac",
          "properties": {
            "folder_separator_icon": "",
            "style": "full"
          },
          "style": "plain",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue753 {{ .Full }})",
          "foreground": "#1976d2",
          "type": "angular",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\uebd8 {{ .EnvironmentName }})",
          "foreground": "#9ec3f0",
          "type": "az",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\uf308 {{ .Context }})",
          "foreground": "#0B59E7",
          "type": "docker",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\uf838 {{ .Version }})",
          "foreground": "#5C2D91",
          "type": "dotnet",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue718 {{ .Full }})",
          "foreground": "#6CA35E",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue7ba {{ .Full }})",
          "foreground": "#81a1c1",
          "type": "react",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue235 {{ .Full }})",
          "foreground": "#906cff",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "branch_ahead_icon": "<#88C0D0>\u21e1 </>",
            "branch_behind_icon": "<#88C0D0>\u21e3 </>",
            "branch_icon": "",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": ""
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#FFAFD7>*</>{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#ff6715",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin"
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "#d633f3",
          "type": "executiontime",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "#00ffff",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}#06b2b2{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
