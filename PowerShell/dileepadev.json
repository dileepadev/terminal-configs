{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}(Admin){{end}} {{.PWD}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf1fa dileepadev:",
          "foreground": "#15aefa",
          "type": "session",
          "style": "plain"
        },
        {
          "foreground": "#fa5fac",
          "properties": {
            "folder_separator_icon": "",
            "style": "full"
          },
          "style": "plain",
          "template": " (\uf07b {{ .Path }})",
          "type": "path"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " (\ue753 {{ .Full }})",
          "foreground": "#1976d2",
          "type": "angular",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " (\uebd8 {{ .EnvironmentName }})",
          "foreground": "#9ec3f0",
          "type": "az",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " (\uf308 {{ .Context }})",
          "foreground": "#0B59E7",
          "type": "docker",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " (\uf838 {{ .Version }})",
          "foreground": "#5C2D91",
          "type": "dotnet",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " (\ue718 {{ .Full }})",
          "foreground": "#6CA35E",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " (\ue7ba {{ .Full }})",
          "foreground": "#81a1c1",
          "type": "react",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_virtual_env": true,
            "display_mode": "environment"
          },
          "template": " (\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }})",
          "foreground": "#6ca7ff",
          "type": "python",
          "style": "plain"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#ff6715",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "display_upstream_icon": true,
            "github_icon": "\uF408",
            "gitlab_icon": "\uF296",
            "bitbucket_icon": "\uF171",
            "azure_devops_icon": "\uEBE8",
            "git_icon": "\uE5FB"
          },
          "template": "\n({{ if .UpstreamIcon }}{{ .UpstreamIcon }} UPSTREAM{{ end }}{{ if .RepoName }} | \uE5FB REPO: {{ .RepoName }}{{ end }} | BRANCH: {{ .HEAD }}{{ if or (gt .Ahead 0) (gt .Behind 0) }} | {{ if gt .Ahead 0 }}\u2191 AHEAD: {{ .Ahead }}{{ end }}{{ if and (gt .Ahead 0) (gt .Behind 0) }} , {{ end }}{{ if gt .Behind 0 }}\u2193 BEHIND: {{ .Behind }}{{ end }}{{ end }}{{ if and (eq .Ahead 0) (eq .Behind 0) }} | \u2714 CLEAN{{ end }}{{ if .Working.Changed }} | \uf044 WD: {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} | \uf046 STAGED: {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} | \uf0c7 STASH: {{ .StashCount }}{{ end }})"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#33f35d",
          "properties": {
            "cache_duration": "none",
            "style": "austin"
          },
          "template": " (\udb84\udfab {{ .FormattedMs }}) "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\udb81\udfb7 ",
          "foreground": "#00ffff",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}#06b2b2{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
